<containers>
    <container>
        <name>{{dashed}}</name>
        <contextItemName>[BBHOST]</contextItemName>
        <properties>
            <property name="TemplateName">
                <value type="string">{{camelCase}}</value>
            </property>
            <property name="thumbnailUrl">
                <value type="string">$(contextRoot)/static/backbase.com.launchpad/media/icons/pagedesigner-layouts-icons_launchpad.png</value>
            </property>
            <property name="title">
                <value type="string">{{title}}</value>
            </property>
            <property name="behaviors" readonly="false">
                <value type="string">be.portalClientExt.PortalManagerPreferencesForm</value>
            </property>
        </properties>
        <tags>
            <tag>launchpad</tag>
            <tag>page structure</tag>
        </tags>
    </container>
</containers>